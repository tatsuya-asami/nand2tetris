/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
        Not(in=sel, out=notSel);
        Or(a=a, b=sel, out=sel1);
        Or(a=b, b=notSel, out=sel2);
        And(a=sel1, b=sel2 , out=out); 
}