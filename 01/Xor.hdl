/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=notA);
    Not(in=b , out=notB);
    And(a=a , b=notB , out=outAndA);
    And(a=notA, b=b , out=outAndB);
    Or(a=outAndA, b=outAndB, out=out);

    // Put your code here:
}